<div class="user-posts">
    {{#each blogs}}
        <a class="user-post" href="/dashboard/edit/{{id}}">
            <span>{{name}}</span>
            <span>{{format_date date_created}}</span>
        </a>
    {{/each}}
</div>
<div class="new-post-btn">
        <a href="/dashboard/new">+ New Post</a>
</div>